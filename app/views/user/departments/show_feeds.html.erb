<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @department.name %>
</p>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @department.feeds.each do |feed| %>
      <tr>
        <td><%= feed.title %></td>
        <td><%= feed.description %></td>
        <td><%= feed.department_id %></td>
        <td><%= link_to 'Show', admin_feed_path(feed) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', user_departments_path %>

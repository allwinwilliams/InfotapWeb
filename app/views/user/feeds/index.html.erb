<p id="notice"><%= notice %></p>

<h1>Listing Feeds</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= feed.title %></td>
        <td><%= feed.description %></td>
        <td><%= feed.department_id %></td>
        <td>
          <%= link_to 'Show', feed %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
